machine bowl1

NAT size := 0

invariants
  @inv size <= 10

events
  event put
  	any x
  	where
  		@grd x : 1..5
  		@grd2 size + x <= 10
  	then
  		@act size := size + x
  end
  
  event take
  	any x
  	where
  		@grd x : 1..5
  		@grd2 size - x >= 0
  	then
  		@act size := size - x
  end
 

end
